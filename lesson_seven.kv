
<Lesson_seven>:
    BoxLayout:
        orientation:'vertical'
        MDToolbar:
            id : toolbar
            md_bg_color: .2, .2, .2, 1
            specific_text_color: 1, 1, 1, 1
            MDRoundFlatButton:
                id:exit
                pos_hint: {"center_x": .9, "center_y": .54}
                on_press:
                    root.manager.transition.direction = 'right'
                    root.manager.current = 'mainscreen'
                    root.stop()
                    root.clean()

        ScreenManager:
            id: sm
            Screen:
                id: plus
                name: 'screen_one'
                BoxLayout:
                    orientation: 'vertical'
                    padding: [10,0,-10,5]
                    spacing: 0
                    MDLabel:
                        id: first_text
                    MDLabel:
                        id: second_text
                    Image:
                        source: "Image/26+.jpg"
                        size_hint: 1, None
                    MDLabel:
                        id: third_text
                    MDLabel:
                        id: fourth_text
                    Image:
                        source: "Image/30+.jpg"
                        size_hint: 1, None
                    MDLabel:
                        id: fifth_text
                    MDFillRoundFlatIconButton:
                        id: next_one
                        icon:"arrow-down"
                        on_press:
                            sm.transition.direction = 'up'
                            sm.current = 'screen_two'
                MDIconButton:
                    id: button_sound
                    icon:  "volume-high"
                    user_font_size: "48sp"
                    pos_hint: {'x': 0.9, 'y': 0.9}
                    on_press: root.play_sound()
            Screen:
                name: 'screen_two'
